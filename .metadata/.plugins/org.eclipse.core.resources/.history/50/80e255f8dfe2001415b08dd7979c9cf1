package exercicio;

public class DadoDoEmpregado {
	/*
	 * Classe que contém dados do funcionario.
	 * 
	 * @author Andrei Luís Amancio
	 */
	
	public static void main(String[] args) { 
		 
		System.out.println("--- DADOS DO EMPREGADO--- ");
		
		 String nome = "VAGABUNDO !";
		 String funcao= "aguma coisa ele faz";
		 int numMatricula = 152264;
		 String admDATA = "14/08/2000 e nunca";
		 String salario = "?";
		 
		 
		 System.out.println("Nome do funcionario:");
		 System.out.println(nome);
		 
		 System.out.println("Sua função na empresa:");
		 System.out.println(funcao);
		 
		 System.out.println("O numero de sua matricula:");
		 System.out.println(numMatricula);
	
		 System.out.println("Sua data de adimição:");
		 System.out.println(admDATA);
		 
		 System.out.println("Seu salario:");
		 System.out.println(salario);
	}

}
