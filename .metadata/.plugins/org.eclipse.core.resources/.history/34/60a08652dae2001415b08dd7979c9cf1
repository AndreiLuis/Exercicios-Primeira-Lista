package exercicio;

public class RegistroDeAgenda {
	
	/*Classe que contem uma agenda com dados dasd pessoas.
	 * 
	 * @author Andrei Luís Amancio
	 */
	
	public static void main(String[] args) { 
		 
		System.out.println("/n/t AGENDA DE CONTATO /n/t")
		
		 String nome = "ninja da padaria";
		 String endereço = "ruas dos louco n°100";
		 int numTelefone = 93554682;
		 String email = "nãosei@hotmail.com";
		 
		 System.out.println("Nome da pessoa :");
		 System.out.println(nome);
		 
		 System.out.println("Seu endereço:");
		 System.out.println(endereço);
		 
		 System.out.println("Seu E-mail:");
		 System.out.println(email);
	
		 System.out.println("Seu telefone:");
		 System.out.println(numTelefone);
	}

}
